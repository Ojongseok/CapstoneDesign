<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_home">

    <fragment
        android:id="@+id/fragment_main"
        android:name="com.example.capstonedesign.view.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" />
    <fragment
        android:id="@+id/fragment_plants_inspect"
        android:name="com.example.capstonedesign.view.main.inspect.PlantsInspectFragment"
        android:label="fragment_plants_inspect"
        tools:layout="@layout/fragment_plants_inspect" >
        <action
            android:id="@+id/action_fragment_plants_inspect_to_inspectResultFragment"
            app:destination="@id/fragment_inspect_result"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <dialog
        android:id="@+id/dialog_bottom_sheet"
        android:name="com.example.capstonedesign.BottomSheetDialog"
        android:label="dialog_bottom_sheet"
        tools:layout="@layout/dialog_bottom_sheet">
        <action
            android:id="@+id/action_dialog_bottom_sheet_to_fragment_plants_inspect"
            app:destination="@id/fragment_plants_inspect"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_dialog_bottom_sheet_to_fragment_post_write"
            app:destination="@id/fragment_post_write"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_dialog_bottom_sheet_to_fragment_search_disease"
            app:destination="@id/fragment_search_crop_info"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </dialog>
    <fragment
        android:id="@+id/fragment_board"
        android:name="com.example.capstonedesign.view.board.BoardFragment"
        android:label="fragment_board"
        tools:layout="@layout/fragment_board">
        <action
            android:id="@+id/action_fragment_board_to_dialog_bottom_sheet"
            app:destination="@id/dialog_bottom_sheet" />
        <action
            android:id="@+id/action_fragment_board_to_fragment_post_detail"
            app:destination="@id/fragment_post_detail"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_fragment_board_to_fragment_mypage"
            app:destination="@id/fragment_mypage"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragment_home"
        android:name="com.example.capstonedesign.view.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_fragment_home_to_dialog_bottom_sheet"
            app:destination="@id/dialog_bottom_sheet" />
    </fragment>
    <fragment
        android:id="@+id/fragment_inspect_result"
        android:name="com.example.capstonedesign.view.main.inspect.InspectResultFragment"
        android:label="fragment_inspect_result"
        tools:layout="@layout/fragment_inspect_result">
        <argument
            android:name="img"
            app:argType="android.net.Uri" />
        <action
            android:id="@+id/action_fragment_inspect_result_to_fragment_home"
            app:destination="@id/fragment_home"
            app:popUpTo="@id/fragment_home"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fragment_post_write"
        android:name="com.example.capstonedesign.view.board.PostWriteFragment"
        android:label="fragment_post_write"
        tools:layout="@layout/fragment_post_write" />
    <fragment
        android:id="@+id/fragment_post_detail"
        android:name="com.example.capstonedesign.view.board.PostDetailFragment"
        android:label="fragment_post_detail"
        tools:layout="@layout/fragment_post_detail" />
    <fragment
        android:id="@+id/fragment_mypage"
        android:name="com.example.capstonedesign.view.board.MyPageFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_my_page" />
    <fragment
        android:id="@+id/fragment_search_crop_info"
        android:name="com.example.capstonedesign.view.main.SearchCropInfoFragment"
        android:label="fragment_search_disease"
        tools:layout="@layout/fragment_search_crop_info" >
        <action
            android:id="@+id/action_fragment_search_crop_info_to_fragment_crop_detail_info"
            app:destination="@id/fragment_crop_detail_info"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragment_crop_detail_info"
        android:name="com.example.capstonedesign.view.main.CropDetailInfoFragment"
        android:label="fragment_crop_detail_info"
        tools:layout="@layout/fragment_crop_detail_info">
        <argument
            android:name="crop_name"
            app:argType="string" />
        <action
            android:id="@+id/action_fragment_crop_detail_info_to_fragment_disease_detail"
            app:destination="@id/fragment_disease_detail"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragment_disease_detail"
        android:name="com.example.capstonedesign.view.main.inspect.DiseaseDetailFragment"
        android:label="fragment_disease_detail"
        tools:layout="@layout/fragment_disease_detail" >
        <argument
            android:name="sickKey"
            app:argType="string" />
    </fragment>
</navigation>